{
  "name": "togglehs",
  "displayName": "Toggle Header/Source",
  "description": "Quickly switch between header/source files for C and C++ files",
  "version": "0.2.0",
  "publisher": "bbenoist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbenoist/vscode-togglehs/issues"
  },
  "homepage": "https://github.com/bbenoist/vscode-togglehs/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/bbenoist/vscode-togglehs.git"
  },
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:togglehs.toggleHS"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "togglehs.toggleHS",
        "title": "Toggle Header/Source"
      }
    ],
    "keybindings": [
      {
        "command": "togglehs.toggleHS",
        "key": "F4"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.45",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.18.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "file-exists": "^1.0.0"
  }
}
